<templateSet group="JavaScript">
  <template name="mtest" value="it(&quot;should return $EXPECTION$ given MarsRover with 00N received $COMMAND$ command&quot;, function () {&#10;      marsRover.execute(&quot;$COMMAND$&quot;);&#10;      expect(marsRover.status).toBe(&quot;$EXPECTION$&quot;);&#10;    });" description="create test for marsRover" toReformat="true" toShortenFQNames="true">
    <variable name="EXPECTION" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="COMMAND" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="cl" value="class $CLASS_NAME$ {&#10;  constructor($PARAMETER$) {&#10;    'ngInject';&#10;&#10;    this.$PARAMETER$ = $PARAMETER$;&#10;  }&#10;&#10;&#10;}&#10;&#10;export default $CLASS_NAME$;&#10;" description="New class with constructor" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="" defaultValue="Class" alwaysStopAt="true" />
    <variable name="PARAMETER" expression="" defaultValue="$" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="desc" value="describe('$TESTNAME$', function () {&#10;    $END$&#10;});" description="create new 'describe' function, group of tests" toReformat="false" toShortenFQNames="true">
    <variable name="TESTNAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="it" value="it('$STATE$', function (done) {&#10;    $END$&#10;    done();&#10;});" description="create new 'it' function, implementation one test" toReformat="false" toShortenFQNames="true">
    <variable name="STATE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>